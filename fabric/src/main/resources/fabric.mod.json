{
    "schemaVersion": 1,
    "id": "plasmo_voice",
    "version": "${version}",
    "name": "RoyalPatreons Voice",
    "description": "Un chat de proximidad compatible con los servidores creador por RoyalPatreons.",
    "authors": [
        "RoyalPatreons"
    ],
    "contact": {
        "homepage": "https://www.royalpatreons.es/",
        "sources": "https://www.royalpatreons.es/",
        "issues": "https://www.royalpatreons.es/"
    },
    "license": "Todos los derechos reservados",
    "icon": "assets/plasmo_voice/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            "su.plo.voice.client.VoiceClientFabric"
        ],
        "main": [
            "su.plo.voice.server.VoiceServerFabric"
        ]
    },
    "mixins": [
        "plasmovoice-common.mixins.json",
        "plasmovoice.mixins.json"
    ],
    "depends": {
        "fabric": ">=${fabric_min_version}",
        "fabricloader": ">=${loader_version}"
    },
    "suggests": {
        "soundphysics": "*"
    }
}
